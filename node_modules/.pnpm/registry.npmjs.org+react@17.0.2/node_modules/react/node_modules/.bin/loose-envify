#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules:/Users/lpuccini/Desktop/Dev/node_modules:/Users/lpuccini/Desktop/node_modules:/Users/lpuccini/node_modules:/Users/node_modules:/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+react@17.0.2/node_modules/react/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+react@17.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules:/Users/lpuccini/Desktop/Dev/node_modules:/Users/lpuccini/Desktop/node_modules:/Users/lpuccini/node_modules:/Users/node_modules:/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+react@17.0.2/node_modules/react/node_modules:/Users/lpuccini/Desktop/Dev/bit/node_modules/.pnpm/registry.npmjs.org+react@17.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
